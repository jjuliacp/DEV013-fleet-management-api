generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model taxis {
  id           Int            @id
  plate        String         @db.VarChar
  trajectories trajectories[]
}

model trajectories {
  taxi_id   Int
  date      DateTime? @db.Timestamp(6)
  latitude  Float?
  longitude Float?
  id        Int       @id @default(autoincrement())
  taxis     taxis     @relation(fields: [taxi_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "trajectories_id_taxis_fkey")
}
